<%= render "layouts/errors", :object => @user %>

<%= form_for @user do |f| %>
  <%= f.label :email %>
  <%= f.text_area :email %>
  <%= f.label :password %>
  <%= f.text_field :password %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit %>
<% end %>
