<h1>Users</h1>

<% if @users.any? %>

    <% @users.each do |user| %>
      <p><%= link_to user.email, user_path(user) %></p>
    <% end %>

<% else %>
  <p>There are no users yet.</p>
<% end %>


<p><%= link_to "Create new user", new_user_path %></p>
<p><%= link_to "See all questions", questions_path %></p>
